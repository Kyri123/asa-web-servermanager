{
	"name": "asa-web-servermanager",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build": "nuxi build",
		"lint": "eslint . --fix && nuxi typecheck",
		"db:push": "dotenv drizzle-kit push:mysql",
		"db:studio": "dotenv drizzle-kit studio",
		"db:generate": "dotenv drizzle-kit generate:mysql",
		"dev": "yarn run db:generate && nuxi dev",
		"start": "node .output/server/index.mjs",
		"prepare": "husky install && nuxi prepare",
		"postinstall": "yarn run prepare",
		"hook:prettier": "prettier --write .",
		"hook:db": "yarn run db:generate"
	},
